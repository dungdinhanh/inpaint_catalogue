        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },

        {
            "name": "Debug mask_gen.py on GPU 0",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/powerpaint/datasets/mask_generator/mask_gen.py",
